<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    package="com.example.joeribes.joeribes_pset4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Many To-Do Lists">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DescriptionActivity"
            android:label="Description To-Do item" />
        <activity
            android:name=".AddActivity"
            android:label="Add your To-Do item" />
        <activity
            android:name=".EditActivity"
            android:label="Edit your To-Do item" />
        <activity
            android:name=".TodoListActivity"
            android:label="To-Do List" />
        <activity
            android:name=".AddTodoListActivity"
            android:label="Add To-Do List" />
        <activity android:name=".EditListActivity"
            android:label="Edit your To-Do list" />

    </application>

</manifest>